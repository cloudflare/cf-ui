{
  "private": true,
  "name": "cf-ui",
  "description": "Cloudflare UI Framework",
  "author": "James Kyle <jkyle@cloudflare.com>",
  "license": "BSD-3-Clause",
  "jest": {
    "testMatch": [
      "**/testj/**/*.js"
    ]
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "./build.sh && npm run build:umd && npm run build:umd:min",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack --config webpack.babel.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack --config webpack.babel.js",
    "build:examples": "rimraf build && cross-env BABEL_ENV=commonjs NODE_ENV=production webpack --config styleguide/config.prod.babel.js && rm build/static-*",
    "dev": "cross-env BABEL_ENV=commonjs gulp dev",
    "lint": "eslint packages/*/src/**/*.js packages/*/example/**/*.js packages/*/test/**/*.js styleguide *.js || true",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "updated": "lerna updated --only-explicit-updates",
    "prettier": "node prettier.js",
    "clean": "lerna clean && rimraf dist",
    "publish": "git pull --rebase && check-installed-dependencies && npm run build && lerna publish --only-explicit-updates",
    "examples:build:internal": "cross-env BABEL_ENV=commonjs gulp examplesBuild --view internal",
    "examples:dev:internal": "cross-env BABEL_ENV=commonjs gulp examplesDev --view internal",
    "examples:build:external": "cross-env BABEL_ENV=commonjs gulp examplesBuild --view external",
    "examples:dev:external": "cross-env BABEL_ENV=commonjs gulp examplesDev --view external",
    "examples:publish": "cross-env BABEL_ENV=commonjs gulp examplesPublish",
    "start": "npm run start:devserver | npm run start:server",
    "start:devserver": "cross-env BABEL_ENV=commonjs node styleguide/server.dev.hook",
    "start:server": "cross-env BABEL_ENV=commonjs NODE_ENV=development node styleguide/server.hook",
    "gen": "node ./styleguide/generateList"
  },
  "devDependencies": {
    "assert-equal-jsx": "^1.0.3",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "7.0.0-alpha.3",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "browserify-istanbul": "^2.0.0",
    "chai": "^3.5.0",
    "check-installed-dependencies": "^1.0.0",
    "compression": "^1.6.2",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.2",
    "cssnano": "^3.5.2",
    "defined": "^1.0.0",
    "enzyme": "^2.7.1",
    "errorify": "^0.3.1",
    "eslint": "^3.8.0",
    "eslint-plugin-prettier": "^2.0.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "fela": "^4.2.5",
    "fela-beautifier": "^4.2.5",
    "fela-font-renderer": "^4.2.5",
    "fela-monolithic": "^4.2.6",
    "fela-plugin-fallback-value": "^4.2.5",
    "fela-plugin-lvha": "^4.2.5",
    "fela-plugin-prefixer": "^4.2.5",
    "fela-plugin-unit": "^4.2.5",
    "fela-plugin-validator": "^4.2.5",
    "file-loader": "^0.10.1",
    "glob": "^7.0.3",
    "gulp": "gulpjs/gulp#4.0",
    "highlight-loader": "^0.7.2",
    "ip": "^1.1.4",
    "isparta": "^4.0.0",
    "jest": "^19.0.2",
    "jsfmt": "^0.5.2",
    "karma": "^1.3.0",
    "karma-beep-reporter": "^0.1.4",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-tape-reporter": "^1.0.0",
    "karma-webpack": "^2.0.2",
    "language-babel": "github:gandm/language-babel",
    "lerna": "2.0.0-beta.37",
    "lodash": "^4.3.0",
    "loose-envify": "^1.3.0",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "prettier": "^0.19.0",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react": "^15.0.0-0",
    "react-addons-css-transition-group": "^15.0.0-0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.0.0-0",
    "react-fela": "^4.2.5",
    "react-redux": "^5.0.3",
    "react-test-renderer": "^15.4.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "static-site-generator-webpack-plugin": "^3.3.0",
    "stream-combiner": "^0.2.2",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0"
  }
}
