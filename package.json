{
  "private": true,
  "name": "cf-ui",
  "description": "Cloudflare UI Framework",
  "author": "James Kyle <jkyle@cloudflare.com>",
  "license": "BSD-3-Clause",
  "jest": {
    "cacheDirectory": ".jest",
    "testMatch": [
      "**/test/**/*.js"
    ],
    "setupFiles": [
      "./test_setup/setup-fetch.js"
    ]
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "./scripts/build.sh",
    "lint": "eslint packages/*/src/**/*.js packages/*/example/**/*.js packages/*/test/**/*.js || true",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "updated": "lerna updated --only-explicit-updates",
    "prettier": "node prettier.js",
    "clean": "lerna clean --yes && rimraf .jest",
    "publish": "git pull --rebase && check-installed-dependencies && npm run build && lerna publish",
    "update-snapshot": "cross-env BABEL_ENV=commonjs jest --no-cache --updateSnapshot"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "check-installed-dependencies": "^1.0.0",
    "cross-env": "^3.2.3",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.17.1",
    "eslint-plugin-prettier": "^2.0.1",
    "fela": "^4.2.6",
    "fela-beautifier": "^4.2.6",
    "fela-font-renderer": "^4.2.6",
    "fela-monolithic": "^4.2.6",
    "fela-plugin-fallback-value": "^4.2.6",
    "fela-plugin-lvha": "^4.2.6",
    "fela-plugin-prefixer": "^4.2.6",
    "fela-plugin-unit": "^4.2.6",
    "fela-plugin-validator": "^4.2.6",
    "glob": "^7.0.3",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^19.0.2",
    "jest-fetch-mock": "^1.0.8",
    "jsdom": "^9.11.0",
    "lerna": "2.0.0-beta.38",
    "prettier": "^0.22.0",
    "react": "^15.0.0-0",
    "react-addons-css-transition-group": "^15.0.0-0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.0.0-0",
    "react-fela": "^4.2.6",
    "react-redux": "^5.0.3",
    "react-test-renderer": "^15.4.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1"
  }
}
