{
  "private": true,
  "name": "cf-ui",
  "description": "CloudFlare UI Framework",
  "author": "James Kyle <jkyle@cloudflare.com>",
  "license": "BSD-3-Clause",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "gulp build",
    "dev": "gulp dev",
    "lint": "eslint packages/*/src/**/*.js packages/*/example/**/*.js packages/*/test/**/*.js styleguide *.js || true",
    "test": "gulp test --browsers PhantomJS",
    "updated": "lerna updated --only-explicit-updates",
    "prettier": "node prettier.js",
    "clean": "lerna clean && rimraf dist",
    "publish": "git pull --rebase && check-installed-dependencies && npm run build && lerna publish --only-explicit-updates",
    "examples:build:internal": "gulp examplesBuild --view internal",
    "examples:dev:internal": "gulp examplesDev --view internal",
    "examples:build:external": "gulp examplesBuild --view external",
    "examples:dev:external": "gulp examplesDev --view external",
    "examples:publish": "gulp examplesPublish",
    "start": "node styleguide/server.dev.hook | NODE_ENV=development node styleguide/server.hook",
    "examples:new": "rimraf build && NODE_ENV=production webpack --config styleguide/config.prod.babel.js && rm build/static-*"
  },
